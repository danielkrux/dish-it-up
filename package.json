  "scripts": {
    "start": "npx expo start --dev-client",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "eas-build:preview": "npx eas build --platform all --profile preview",
    "eas-build:android:preview": "npx eas build --platform android --profile preview",
    "eas-build:ios:preview": "npx eas build --platform ios --profile preview",
    "eas-build:ios:production": "npx eas build --platform ios --profile production",
    "update": "eas update --branch main --message x",
    "update:production": "eas update --branch production --message: 'x'",
    "prebuild": "npx expo prebuild",
    "api:serve": "supabase functions serve",
    "api:deploy": "supabase functions deploy",
    "generate": "supabase gen types typescript --project-id qfsraezjniacsdtipuzs > supabase/database.types.ts",
    "postinstall": "patch-package",
    "deploy": "netlify deploy --dir dist --prod",
    "predeploy": "expo export -p web"
  },
